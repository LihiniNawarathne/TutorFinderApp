<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".StudentUI.registerStudent1">

    <TextView
        android:id="@+id/tvThemeR"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:layout_marginBottom="44dp"
        android:text="@string/register"
        android:textColor="@color/SplashScreenBackground"
        android:textSize="36sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/rtil1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />


    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/rtil1"
        android:layout_width="315dp"
        android:layout_height="0dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/textview_border"
        android:textColorHint="#B9B8B8"
        app:layout_constraintBottom_toTopOf="@+id/rtil2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvThemeR">

        <EditText
            android:id="@+id/etv_Fname"
            android:layout_width="300dp"
            android:layout_height="51dp"
            android:layout_marginLeft="10dp"
            android:autofillHints=""
            android:ems="10"
            android:hint="@string/Fname"
            android:inputType="textPersonName"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="100dp"
            tools:layout_editor_absoluteY="292dp" />
    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/rtil2"
        android:layout_width="315dp"
        android:layout_height="0dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/textview_border"
        android:textColorHint="#B9B8B8"
        app:layout_constraintBottom_toTopOf="@+id/rtil3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rtil1">

        <EditText
            android:id="@+id/etv_phone"
            android:layout_width="300dp"
            android:layout_height="51dp"
            android:layout_marginLeft="10dp"
            android:autofillHints=""
            android:ems="10"
            android:hint="@string/rphone"
            android:inputType="number"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="100dp"
            tools:layout_editor_absoluteY="292dp" />
    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/rtil3"
        android:layout_width="315dp"
        android:layout_height="0dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/textview_border"
        android:textColorHint="#B9B8B8"
        app:layout_constraintBottom_toTopOf="@+id/rtil4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rtil2">

        <EditText
            android:id="@+id/etv_remail"
            android:layout_width="300dp"
            android:layout_height="51dp"
            android:layout_marginLeft="10dp"
            android:autofillHints=""
            android:ems="10"
            android:hint="@string/remail"
            android:inputType="textEmailAddress"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="100dp"
            tools:layout_editor_absoluteY="292dp" />
    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/rtil4"
        android:layout_width="315dp"
        android:layout_height="0dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/textview_border"
        android:textColorHint="#B9B8B8"
        app:layout_constraintBottom_toTopOf="@+id/rtil5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rtil3">

        <EditText
            android:id="@+id/etv_schl"
            android:layout_width="300dp"
            android:layout_height="51dp"
            android:layout_marginLeft="10dp"
            android:autofillHints=""
            android:ems="10"
            android:hint="@string/rschool"
            android:inputType="textPersonName"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="100dp"
            tools:layout_editor_absoluteY="292dp" />
    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/rtil5"
        android:layout_width="315dp"
        android:layout_height="0dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/textview_border"
        android:textColorHint="#B9B8B8"
        app:layout_constraintBottom_toTopOf="@+id/rtil6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rtil4">

        <EditText
            android:id="@+id/etv_sNIC"
            android:layout_width="300dp"
            android:layout_height="51dp"
            android:layout_marginLeft="10dp"
            android:autofillHints=""
            android:ems="10"
            android:hint="@string/rNIC"
            android:inputType="textPersonName"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="100dp"
            tools:layout_editor_absoluteY="292dp" />
    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/rtil6"
        android:layout_width="315dp"
        android:layout_height="0dp"
        android:layout_marginBottom="40dp"
        android:background="@drawable/textview_border"
        android:textColorHint="#B9B8B8"
        app:layout_constraintBottom_toTopOf="@+id/btnReg"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rtil5">

        <EditText
            android:id="@+id/etv_Alstrm"
            android:layout_width="300dp"
            android:layout_height="51dp"
            android:layout_marginLeft="10dp"
            android:autofillHints=""
            android:ems="10"
            android:hint="@string/rstream"
            android:inputType="textPersonName"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="100dp"
            tools:layout_editor_absoluteY="292dp" />
    </com.google.android.material.textfield.TextInputLayout>

    <Button
        android:id="@+id/btnReg"

        android:layout_width="161dp"
        android:layout_height="0dp"

        android:layout_marginBottom="18dp"
        android:background="@drawable/textview_border"
        android:backgroundTint="@color/ScreenBackground"
        android:text="@string/btnregisterNext"
        android:textColor="@color/white"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rtil6"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>